{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": [
      "off",
      { "groups": [["^\\u0000", "^node:", "^@?\\w", "^", "^\\."]] }
    ],
    "simple-import-sort/exports": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
